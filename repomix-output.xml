<file path="packages/prisma/schema/models/creature/creature.prisma">
model Creature {
  id          Int       @id @default(autoincrement())
  name        String    @unique
  level       Int       @default(1)
  areaId      Int?      // 外鍵，關聯到 Area
  area        Area?     @relation(fields: [areaId], references: [id])
  attackType  AttackType
  bodyType    BodyType
  description String?

  monster     Monster?
  pet         Pet?

  @@index([name, level]) // 按名稱和等級查詢生物
}
</file>

<file path="packages/prisma/schema/models/creature/monster.prisma">
model Monster {
  id          Int       @id @default(autoincrement())
  creature    Creature  @relation(fields: [creatureId], references: [id])
  creatureId  Int      @unique // 確保一對一關係
  
  drops       MonsterDrop[]
  attackMode  AttackMode // 攻擊類型，主動或被動
  monsterStage MonsterStage // 怪物階段
  dungeonMonster DungeonMonster?
  canCapture  Boolean    @default(true)
  petEgg      Item?      @relation("MonsterToPetEgg", fields: [petEggId], references: [id])
  petEggId    Int?
}
</file>

<file path="packages/prisma/schema/models/creature/pet-attribute.prisma">
model PetAttribute {
  id        Int        @id @default(autoincrement())
  petId     Int
  attribute Attribute
  value Float
  unit      UnitType?

  pet       Pet        @relation(fields: [petId], references: [id])
  @@unique([petId, attribute]) // 同一隻寵物不重複同一屬性
}
</file>

<file path="packages/prisma/schema/models/creature/pet.prisma">
model Pet {
  id          Int       @id @default(autoincrement())
  creature    Creature  @relation(fields: [creatureId], references: [id])
  creatureId  Int      @unique // 確保一對一關係
  
  obtainType  PetObtainType
  obtainWay   String?
  bodyType    BodyType
  
  skills   PetSkillLink[] // 更新為中介模型
  
  attributes PetAttribute[]
  quality     ItemQuality
  petEgg      Item      @relation("PetToEgg", fields: [petEggId], references: [id])
  petEggId    Int
  petEggDetail PetEggDetail? @relation("PetEggDetailRelation")
}
</file>

<file path="packages/prisma/schema/models/geography/area.prisma">
model Area {
