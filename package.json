{
  "name": "glory-destiny-online-guide",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@10.6.4",
  "scripts": {
    "build": "pnpm run -r build",
    "lint": "pnpm run -r lint",
    "test": "pnpm run -r test",
    "dev": "docker-compose -f docker-compose.dev.yaml up --build",
    "prisma:generate": "pnpm --filter @glory-destiny-online-guide/prisma run generate",
    "prisma:migrate": "pnpm --filter @glory-destiny-online-guide/prisma run migrate:dev",
    "clean": "pnpm -r exec rm -rf node_modules dist .next generated"
  },
  "devDependencies": {
    "@eslint/js": "latest",
    "@swc-node/register": "1.10.10",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.10.7",
    "@tailwindcss/postcss": "^4.1.3",
    "@types/node": "^22.14.0",
    "@typescript-eslint/eslint-plugin": "^8.29.1",
    "@typescript-eslint/parser": "^8.29.1",
    "eslint": "9.24.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.6",
    "eslint-plugin-react": "latest",
    "eslint-plugin-react-hooks": "latest",
    "nodemon": "^3.1.9",
    "postcss": "^8.5.3",
    "prettier": "latest",
    "tailwindcss": "^4.1.3",
    "ts-loader": "^9.5.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "5.8.3",
    "typescript-eslint": "latest",
    "@turbo/gen": "^2.5.0"
  }
}
